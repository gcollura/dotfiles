# personal account
Host github.com-gcollura
	IgnoreUnknown UseKeychain
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_personal
	UseKeychain yes
	AddKeysToAgent yes
	IdentitiesOnly yes

# opal account
Host github.com-opal
	IgnoreUnknown UseKeychain
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_opal
	UseKeychain yes
	AddKeysToAgent yes
	IdentitiesOnly yes
